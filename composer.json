{
  "name": "storytile/json-structure",
  "description": "A class to define JSON structures, with an integrated way to map JSON data onto the class properties",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Storytile\\JsonStructure\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}